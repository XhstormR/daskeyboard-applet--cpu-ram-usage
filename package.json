{
  "name": "daskeyboard-applet--cpu-ram-usage",
  "version": "1.0.0",
  "displayName": "CPU | RAM Usage",
  "description": "Displays CPU or RAM usage.",
  "icon": "assets/icon.png",
  "publisher": "XhstormR",
  "authorName": "XhstormR",
  "authorUrl": "https://github.com/XhstormR",
  "issuesUrl": "https://github.com/XhstormR",
  "homePageUrl": "https://github.com/XhstormR",
  "developerRepoUrl": "https://github.com/XhstormR",
  "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.txt",
  "changelogUrl": "CHANGELOG.md",
  "readMeUrl": "README.md",
  "readMeEndUserUrl": "README_ENDUSER.md",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js"
  },
  "dependencies": {
    "daskeyboard-applet": "^2.11.4",
    "lodash": "^4.17.15"
  },
  "engines": {
    "das-keyboard-q": "3.0.0"
  },
  "qConfig": {
    "geometry": {
      "width": 10,
      "height": 1,
      "defaults": {
        "origin": {
          "x": 0,
          "y": 0
        }
      }
    },
    "questions": [
      {
        "key": "displayType",
        "label": "Type",
        "required": true,
        "order": 1,
        "controlType": "dropdown",
        "options": [
          {
            "key": "CPU",
            "value": "CPU"
          },
          {
            "key": "RAM",
            "value": "RAM"
          }
        ]
      }
    ]
  }
}
